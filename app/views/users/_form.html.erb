<%=form_for @user, :url => user_path do |f|%>
	<%=f.label :first_name%>
	<%=f.text_field :first_name%>
	<%=f.label :last_name%>
	<%=f.text_field :last_name%>
	<%=f.label :img_url%>
	<%=f.text_field :img_url%>
	<%=f.label :bio%>
	<%=f.text_field :bio%>
	<%=f.label :email%>
	<%=f.text_field :email%>
	<%=f.label :password%>
	<%=f.password_field :password%>
	<%=f.label :password_confirmation%>
	<%=f.password_field :password_confirmation%>
	<%=f.submit "Submit"%>
<%end%>
