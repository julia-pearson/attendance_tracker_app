<%= form_for @cohort do |f| %>
  <%= f.label  :cohort_name %>
  <%= f.text_field :cohort_name %>
  <%= f.label    :start_date %>
  <%= f.date_field :start_date %>
  <%= f.label    :end_date %>
  <%= f.date_field :end_date %>
  <%= f.label  :subject %>
  <%= f.text_field :subject  %>
  <%= f.label :full_time %>
  <%= f.check_box :full_time %>
  <%= f.label :max_absences %>
  <%= f.number_field :max_absences %>
  <input id="second-instructor-email"type="text" name="second_instructor_email" placeholder="Second Instructor">
  <input id="third-instructor-email-2"type="text" name="third_instructor_email" placeholder="Third Instructor">
  <input id="producer-email"type="text" name="producer_email" placeholder="Producer">
  <%= f.submit %>
<% end %>