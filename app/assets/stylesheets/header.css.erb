nav {
	justify-content: space-between;
	align-items: center;
  background-color: rgba(255,255,255, 0.75);
}

ul.nav-links {
	display: flex;
	list-style-type: none;
  border: solid;
  border-color: rgb(255,255,255);
  border-width: 3px;
  margin-bottom: 30px;
  margin-top: 50px;
  padding: 10px;
  text-decoration: none;
}

ul.nav-links li {
	font-size: 30px;
}
a{
  text-decoration: none;
  color: rgb(255, 80, 80);
}